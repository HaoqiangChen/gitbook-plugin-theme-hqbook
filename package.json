{
  "name": "gitbook-plugin-theme-hqbook",
  "version": "1.1.0",
  "description": "A Gitbook theme for hqbook",
  "main": "index.js",
  "engines": {
    "gitbook": ">=3.0.0"
  },
  "gitbook": {
    "properties": {
      "favicon": {
        "default": "./favicon.ico",
        "title": "Add path to your site favicon in favicon",
        "type": "string"
      },
      "logo": {
        "default": "../../images/home/logo.png",
        "title": "Insert the logo into the top of the navigation bar",
        "type": "string"
      },
      "search-placeholder": {
        "default": "输入关键字搜索",
        "title": "The placeholder of search box",
        "type": "string"
      },
      "copyButtons": {
        "default": true,
        "title": "Adds buttons to copy the commands",
        "type": "boolean"
      },
      "copyLines": {
        "default": true,
        "title": "When displaying code with multiple lines, line numbers will be added",
        "type": "boolean"
      },
      "dragSplitter": {
        "default": true,
        "title": "Add a drag-and-drop bar to adjust the width on both sides",
        "type": "boolean"
      },
      "flexible-linkcard": {
        "type": "object",
        "title": "Converts blockquotes into beautiful linkcard, custom nice links",
        "default": {
          "title": "Converts blockquotes into beautiful linkcard, custom nice links",
          "target": "_blank",
          "hrefUrl": "../../demo/demo.html",
          "imgSrc": "../../images/home/logo.png",
          "imgClass": ""
        }
      }
    }
  },
  "scripts": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HaoqiangChen/gitbook-plugin-theme-hqbook.git"
  },
  "keywords": [
    "gitbook",
    "theme-hqbook",
    "prism"
  ],
  "author": "chenhaoqiang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HaoqiangChen/gitbook-plugin-theme-hqbook/issues"
  },
  "homepage": "https://github.com/HaoqiangChen/gitbook-plugin-theme-hqbook#readme"
}
